    <h1 data-i18n-key="signin">Sign In</h1>

    <div class="form-row col-md-8 login-cred">
        <div class="form-group">
            <label for="FirstName" class="pl-0 field-label" data-i18n-key="login_first_name">First Name (must match voter registration)</label>
            <div class="form-floating">
                <input name="ctl00$cphContent$FirstName" type="text" id="FirstName" class="form-control is-invalid" placeholder="First Name" autocomplete="off" />
                <label for="FirstName" id="FirstNameCheck" class="text-secondary" data-i18n-key="login_required">Required</label>
            </div>
        </div>
    </div>
    <div class="form-row col-md-8 pt-2 login-cred">
        <div class="form-group">
            <label for="LastName" class="pl-0 field-label" data-i18n-key="login_last_name">Last Name (must match voter registration)</label>
            <div class="form-floating">
                <input name="ctl00$cphContent$LastName" type="text" id="LastName" class="form-control is-invalid" placeholder="Last Name" autocomplete="off" />
                <label for="LastName" id="LastNameCheck" class="text-secondary" data-i18n-key="login_required">Required</label>
            </div>
        </div>
    </div>
    <div class="form-row col-md-8 pt-2 login-cred">
        <div class="form-group">
            <label for="BirthDate" class="pl-0 field-label" data-i18n-key="login_date_of_birth">Date of Birth (mm/dd/yyyy)</label>
            <div class="form-floating">
                <input name="ctl00$cphContent$BirthDate" maxlength="10" id="BirthDate" class="form-control is-invalid" placeholder="mm/dd/yyyy" type="tel" autocomplete="off" />
                <label for="BirthDate" id="BirthDateCheck" class="text-secondary" data-i18n-key="login_required">Required</label>
            </div>
        </div>
    </div>

    <div class="form-row col-md-8 pt-3 login-cred">
        <button id="btnLogin" class="btn btn-primary button" onclick="signIn();" type="button"><span data-i18n-key="signin">Sign in</span></button>
    </div>

